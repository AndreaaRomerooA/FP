		// EJERCICIO 1.3 Convertir yardas a metros
		double n2 = 0.9144;
		double yardas;
		Console.WriteLine("Ingrese las yardas: ");
		yardas = (Console.ReadLine());
		metros = yardas * n2;
		Console.WriteLine("Los metros a los que equivale son: "(metros));


		// EJERCICIO 1.4 Depreciación de un auto del 80% de 200,000 por 5 años
		int precio = 200000;
		double depreciar = 0.8;
		int valor1 = 5;
		depreciacion = precio * depreciar;
		int valor2 = depreciacion / valor1;
		Console.WriteLine("La depreciación por será de "(valor2));


		// EJERCICIO 1.5 Deduccion de impuestos del salario 
		int sueldo = 25000;
		int fijo = 900;
		double porcentual = 0.015;
		impuesto = sueldo - fijo * porcentual;
		neto = sueldo - impuesto;
		Console.WriteLine("Impuesto a deducir: $"(impuesto));
		Console.WriteLine("Sueldo neto: $"(neto));


		//EJERCICIO 1.6 Operaciones con variables
		int v1 = 3;
		int v2 = 12;
		double f1 = 3.0;
		// 1
		r1 = v1 * 4;
		Console.WriteLine("Resultado 1 : ");
		Console.WriteLine(r1);
		// 2
		r2 = v2 / v1;
		Console.WriteLine("Resultado 2 : ");
		Console.WriteLine(r2);
		// 3
		r3 = v2 % v1;
		Console.WriteLine("Resultado 3 : ");
		Console.WriteLine(r3);
		// 4
		r4 = v2 % 7;
		Console.WriteLine("Resultado 4 : ");
		Console.WriteLine(r4);
		// 5
		r5 = v1 + f1;
		Console.WriteLine("Resultado 5 : ");
		Console.WriteLine(r5);
		// 6
		r6 = 10 / 100;
		Console.WriteLine("Resultado 6 : ");
		Console.WriteLine(r6);

		
