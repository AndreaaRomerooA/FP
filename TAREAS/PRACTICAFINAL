		// EJERCICIO 1.5
		double cuenta;
		Console.WriteLine("Ingrese el total de la cuenta y el porcentaje de propina a aplicar");
		Console.WriteLine("Total de la cuenta: ");
		cuenta = (Console.ReadLine());
		int porcentaje;
		Console.WriteLine("Porcentaje de propina: ");
		porcentaje = (Console.ReadLine());
		double propina = cuenta + ((porcentaje * cuenta) / 100);
		Console.WriteLine("La cantidad a dar de propina es de "(propina));


