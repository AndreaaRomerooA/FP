        // EJERCICIO 1.22
        int numero1, numero2, total;
        string opcion2;
        do
        {
            Console.WriteLine("Menu de operaciones");
            Console.WriteLine("a) Suma de dos operandos");
            Console.WriteLine("b) Resta de dos operandos");
            Console.WriteLine("c) Multiplicacion de dos operandos");
            Console.WriteLine("d) Division de dos operandos");
            Console.WriteLine("e) Residuo de dos operandos");
            Console.WriteLine("f) Salir del menu");
           opcion2 = (Console.ReadLine());
            Console.WriteLine("Primer numero: ");
            numero1 = (Console.ReadLine());
            Console.WriteLine("Segundo numero: ");
            numero2 = (Console.ReadLine());
            if (opcion2 == "a")
            {
                Console.WriteLine("Resultado de operacion: "(numero1 + numero2));
            }
            if (opcion2 == "b")
            {
                Console.WriteLine("Resultado de operacion: "(numero1 - numero2));
            }
            if (opcion2 == "c")
            {
                Console.WriteLine("Resultado de operacion: "(numero1 * numero2));
            }
            if (opcion2 == "d")
            {
                Console.WriteLine("Resultado de operacion: "(numero1 / numero2));
            }
            if (opcion2 == "e")
            {
                Console.WriteLine("Resultado de operacion: " (numero1 % numero2));
            }
        } while (opcion2 != "f");
        break;


        // EJERCICIO 1.23
        long long numTarjeta, dinero;
        int pin;
        string opcion3;
        Console.WriteLine("Ingrese su numero de tarjeta: ");
        numTarjeta = (Console.ReadLine());
        Console.WriteLine("Ingrese su pin: ");
        pin = (Console.ReadLine());
        do
            {
                if (pin == "1234")
                {
                Console.WriteLine("Menu de operaciones cajero Unitec");
                Console.WriteLine("a Consulta de saldo");
                Console.WriteLine("b Retiro de dinero");
                Console.WriteLine("c Cancelar operacion");
                } else 
                { 
                    Console.WriteLine("Pin incorrecto");
                }
                opcion3 = (Console.ReadLine());
                if (opcion3 == "a")
                {
                    Console.WriteLine("Saldo: $3450.50");
                }
                if (opcion3 == "b")
                {
                    Console.WriteLine("Dinero a retirar: ");
                    dinero = (Console.ReadLine());
                    if (dinero > 3450)
                    { 
                        Console.WriteLine("Fondos insuficientes");
                    }
                    else 
                    { 
                        Console.WriteLine("Saldo remanente: $"(3450.50 - dinero));
                    }
                }   
            }
            while (opcion3 != "c");
        break;

	

