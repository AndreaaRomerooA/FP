		// EJERCICIO 1.9
		float farenheith;
		Console.WriteLine("Ingrese los grados Frenheith: ");
		farenheith = (Console.ReadLine());
		float grados = ((farenheith - 32) * 5) / 9;
		if (farenheith == 32)
		{
			Console.WriteLine("Cero grados centigrados");
		}
		else
		{
			Console.WriteLine(" centigrados");
		}
		

		// EJERCICIO 1.10
		double peso2, estatura;
		double mc;
		Console.WriteLine("Ingresa tu peso: ");
		peso2 = (Console.ReadLine());
		Console.WriteLine("Ingresa tu estatura: ");
		estatura = (Console.ReadLine());
		mc = peso2 / (estatura * estatura);
		Console.WriteLine("Masa corporal: "(mc));
		if (mc < 18.5)
		{
			Console.WriteLine("Abajo del peso normal");
		}
		if (mc >= 18.5 and mc < 25)
		{
			Console.WriteLine("Peso normal");
		}
		if (mc >= 25 and mc < 30)
		{
			Console.WriteLine("Sobrepeso");
		}
		if (mc >= 30 and mc < 35)
		{
			Console.WriteLine("Obeso clase 1");
		}
		if (mc >= 35 and mc < 40)
		{
			Console.WriteLine("Obeso clase 2");
		}
		if (mc >= 40)
		{
			Console.WriteLine("Obeso clase 3");
		}

